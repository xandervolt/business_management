[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=xandervolt
Group=www-data
WorkingDirectory=/home/xandervolt/business_management/business_management/
ExecStart=/home/xandervolt/business_management/businessmanagementenv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/xandervolt/business_management/business_management/business_management.sock config.wsgi:application

[Install]
WantedBy=multi-user.target

